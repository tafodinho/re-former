<p> hello new user </p>


<%= form_for @user do |form| %>
    <%= form.label 'Username' %>
    <%= form.text_field :username %>
    <br />  <br />
    <%= form.label 'Email' %>
    <%= form.email_field :email %>
    <br /> <br />
    <%= form.label 'Password' %>
    <%= form.password_field :password %>
    <br /> <br />
    <%= form.submit "Create User" %>
<% end %>
